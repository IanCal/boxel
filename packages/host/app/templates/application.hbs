{{#if this.model.isServerRender}}
  <ServerRender @url={{this.model.url}} @format={{this.model.format}}/>
{{else}}
  {{page-title 'RuntimeSpike'}}

  <CardCatalogModal />
  <CreateCardModal />

  <Go
    @path={{this.model.path}}
    @openFile={{this.model.openFile}}
    @polling={{this.model.polling}}
  />

  {{outlet}}
{{/if}}